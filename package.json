{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "nodemon server/server.js -w server",
    "dev": "nodemon server/server.js -w server",
    "build:client": "NODE_ENV=production webpack --config webpack-client.config.js --progress --colors -p",
    "build:server": "NODE_ENV=production webpack --config webpack-server.config.js --progress --colors -p",
    "build": "npm run build:client && npm run build:server",
    "server": "node server/server-bundle.js",
    "all": "npm run build && npm run server",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-hot-boilerplate",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-plugin-transform-require-ignore": "0.0.2",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babel-register": "^6.23.0",
    "css-loader": "^0.26.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-loader": "^0.1.2",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "atob": "^2.0.3",
    "axios": "^0.15.3",
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "node-zip": "^1.1.1",
    "react": "^0.14.6",
    "react-async-script-loader": "^0.2.2",
    "react-dom": "^0.14.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-persist": "^4.4.2",
    "redux-thunk": "^2.2.0",
    "svg2png": "^4.1.1"
  }
}
